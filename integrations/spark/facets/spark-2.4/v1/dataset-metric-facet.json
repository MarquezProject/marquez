{
  "$schema": "http://json-schema.org/draft-07/schema",
  "definitions": {
    "DatasetQualityFacet": {
      "type": "object",
      "title": "Data quality facet schema",
      "properties": {
        "/": {}
      },
      "patternProperties": {
        "^[a-zA-Z]+$": {
          "type": "array",
          "items": {
            "$id": "#/properties/example_seconds/items",
            "anyOf": [
              {
                "$ref": "#/definitions/Quantile"
              },
              {
                "$ref": "#/definitions/Count"
              },
              {
                "$ref": "#/definitions/Sum"
              },
              {
                "$ref": "#/definitions/Le"
              }
            ]
          }
        },
        "additionalProperties": false
      }
    },
    "Quantile": {
      "description": "A metric representing a quantile, e.g. 0.99 for 99th percentile",
      "type": "object",
      "properties": {
        "/": {}
      },
      "patternProperties": {
        "^\\d+$": {
          "type": "integer"
        }
      },
      "additionalProperties": false
    },
    "Count": {
      "description": "A counter of the number of events, such as tasks completed or errors",
      "type": "integer"
    },
    "Sum": {
      "description": "The total sum of all observed values",
      "type": "integer"
    },
    "Le": {
      "description": "The upper inclusive bound for all events seen so far. Used for histograms.",
      "type": "object",
      "properties": {
        "/": {}
      },
      "patternProperties": {
        "^\\d+$": {
          "type": "integer"
        }
      },
      "additionalProperties": false
    }
  }
}

